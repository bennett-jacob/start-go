{
	"name": "Existing Docker Compose (Extend)",
	"dockerComposeFile": [
		"../docker-compose.yml",
		"docker-compose.yml"
	],
	"service": "app",
	"workspaceFolder": "/workspace",
	"settings": {
		"sqltools.connections": [
			{
				"previewLimit": 50,
				"server": "postgres",
				"port": 5432,
				"driver": "PostgreSQL",
				"name": "postgres",
				"database": "postgres",
				"username": "postgres",
				"password": "postgres"
			}
		]
	},
	"extensions": [
		"golang.go",
		"adpyke.vscode-sql-formatter",
		"mtxr.sqltools",
		"mtxr.sqltools-driver-pg",
		"rangav.vscode-thunder-client"
	]
}
